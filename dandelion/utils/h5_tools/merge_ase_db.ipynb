    "from ase.db import connect\n",
    "\n",
    "# Connect to the two databases you want to merge\n",
    "db1 = connect('a.db')\n",
    "db2 = connect('b.db')\n",
    "\n",
    "# Iterate over the entries in db2 and add them to db1\n",
    "for row in db2.select():\n",
    "    db1.write(row.toatoms())\n"
   ]
  }
